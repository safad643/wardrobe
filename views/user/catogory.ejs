	<style>
		body {
    overflow-x: hidden;
}

  
  .filter-container {
    background: #fff;
    padding: 20px;
    border-radius: 12px;
    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.05);
    max-width: 300px;
    font-family: 'Segoe UI', sans-serif;
    position: -webkit-sticky;  /* For Safari */
    position: sticky;
    top: 80px;  /* Distance from the top of the viewport */
    z-index: 10;  /* Ensure it stays on top of other content */
}

  
  .filter-header {
      display: flex;
      justify-content: space-between;
      align-items: center;
      margin-bottom: 25px;
      padding-bottom: 15px;
      border-bottom: 1px solid #f0f0f0;
  }
  
  .filter-title {
      font-size: 1.2rem;
      font-weight: 600;
      color: #333;
  }
  
  .clear-all {
      color: #ff4e4e;
      font-size: 0.9rem;
      cursor: pointer;
      border: none;
      background: none;
      transition: color 0.3s;
  }
  
  .clear-all:hover {
      color: red;
  }
  
  .filter-group {
      margin-bottom: 25px;
  }
  
  .filter-group-title {
      font-weight: 600;
      color: #444;
      margin-bottom: 15px;
      font-size: 1rem;
  }
  
  .price-range {
      display: flex;
      gap: 10px;
      margin-bottom: 15px;
  }
  
  .price-input {
      width: 100%;
      padding: 8px;
      border: 1px solid #ddd;
      border-radius: 6px;
      outline: none;
      transition: border-color 0.3s;
  }
  
  .price-input:focus {
      border-color: #ff4d94;
  }
  
  .checkbox-group {
      display: flex;
      flex-direction: column;
      gap: 12px;
  }
  
  .checkbox-item {
      display: flex;
      align-items: center;
      gap: 10px;
      cursor: pointer;
  }
  
  .custom-checkbox {
      width: 20px;
      height: 20px;
      border: 2px solid #ddd;
      border-radius: 4px;
      display: flex;
      align-items: center;
      justify-content: center;
      transition: all 0.3s;
  }
  
  .checkbox-item:hover .custom-checkbox {
      border-color: #ff4d94;
  }
  
  .custom-checkbox.checked {
      background-color: #ff4d94;
      border-color: #ff4d94;
  }
  
  .custom-checkbox.checked::after {
      content: '✓';
      color: white;
      font-size: 14px;
  }
  
  .color-options {
      display: flex;
      gap: 10px;
      flex-wrap: wrap;
  }
  
  .color-option {
      width: 30px;
      height: 30px;
      border-radius: 50%;
      cursor: pointer;
      position: relative;
      transition: transform 0.3s;
  }
  
  .color-option:hover {
      transform: scale(1.1);
  }
  
  .color-option.selected::after {
      content: '';
      position: absolute;
      inset: -3px;
      border: 2px solid #ff4d94;
      border-radius: 50%;
  }
  
  .apply-filters {
      width: 100%;
      padding: 12px;
      background: #000000;
      color: white;
      border: none;
      border-radius: 8px;
      font-weight: 600;
      cursor: pointer;
      transition: background-color 0.3s transform 0.3s;
      margin-top: 20px;
      
  }
  
  .apply-filters:hover {
      background: #2c2c2c;
      transform: scale(1.05);
  }
 
  .rating-filter {
    display: flex;
    align-items: center;
    gap: 10px;
    padding: 10px 0;
}

.stars {
    display: inline-flex;
    flex-direction: row-reverse;
    gap: 3px;
}

.stars input[type="radio"] {
    display: none;
}

.stars label {
    cursor: pointer;
    color: #ddd;
    font-size: 20px;
    transition: color 0.2s ease-in-out;
}

.stars label:hover,
.stars label:hover ~ label,
.stars input[type="radio"]:checked ~ label {
    color: #ffd700;
}

.rating-text {
    color: #666;
    font-size: 14px;
}
.pagetile{
	cursor: pointer;
}

